  </tr>

    {% for item in list %}
      <tr height="27">
        <td height="27">{{ item.snummer }}</td>
        <td>{{ item.contact }}</td>
        <td>{{ item.omschrijving }}</td>
        <td>{{ item.omschrijving }}</td>
        <td>{{ item.startdate }}</td>
        <td>{{ item.aantal }}</td>
        <td>{{ item.kostprijs }}</td>
        <td>{{ item.leveranciers }}</td>
      </tr>
    {% endfor %}
</table>
</body>
