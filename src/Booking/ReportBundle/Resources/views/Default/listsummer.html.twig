  <tr height="54">
    <td width="12%" height="54" bgcolor="#FF9999"><div align="center"><strong>Snummer</strong></div></td>
    <td width="12%" bgcolor="#FF9999"><div align="center"><strong>Contact</strong></div></td>
    <td width="14%" bgcolor="#FF9999"><div align="center"><strong>Omschrijving</strong></div></td>
    <td width="14%" bgcolor="#FF9999"><div align="center"><strong>Omschrijving</strong></div></td>
